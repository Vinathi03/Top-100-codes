#include<stdio.h>
int main(){
    int n,n1,n2;
    scanf("%d%d%d",&n,&n1,&n2);
    if(n>n1&&n>n2)
        printf("%d is greater than %d and %d",n,n1,n2);
    else if(n1>n&&n1>n2)
        printf("%d is greater than %d and %d",n1,n,n2);
    else
        printf("%d is greater than %d and %d",n2,n,n1);
    return 0;    
}
